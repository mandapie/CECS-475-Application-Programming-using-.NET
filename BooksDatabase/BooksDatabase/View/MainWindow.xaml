        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BooksDatabase"
        mc:Ignorable="d"
        Title="MainWindow" Height="407.203" Width="655.932"
        DataContext="{Binding MainViewModel, Source={StaticResource Locator}}">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="312" Margin="41,42,0,0" TextWrapping="Wrap" Text="{Binding Text}" VerticalAlignment="Top" Width="561"/>
    </Grid>
</Window>
